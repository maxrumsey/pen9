<html>
  <head>
    <link rel="stylesheet" type="text/css" href="pen9.css">
    <link href="https://fonts.googleapis.com/css?family=Inconsolata" rel="stylesheet">
  </head>
  <body id='dashboard'>
    <header id='userinfo-header'>
      <a id='normal' href='/settings'>
        <%= user.name %>
      </a>
      <a href='/settings'>Settings</a>
      <a href='/note/new'>Create Note</a>
    </header>
    <div id='dash-notes'>
      <h1>Your Notes</h1>
      <% notes.forEach(function(note) { %>
        <div class='note'>
          <b class='name'><%= note.name %></b>
          <%= note.ageRelative %>
          <a href='/note/<%=note.id%>/edit'>Edit</a>
        </div>
      <% }) %>
    </div>
  </body>
</html>
